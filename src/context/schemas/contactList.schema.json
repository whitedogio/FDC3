{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "https://fdc3.finos.org/schemas/next/contactList.schema.json",
  "type": "object",
  "title": "ContactList",
  "allOf": [
    { "$ref": "context.schema.json" },
    {
      "properties": {
        "type": {
          "const": "fdc3.contactList",
          "tsType": "ContextType"
        },
        "contacts": {
          "type": "array",
          "items": { "$ref": "contact.schema.json" }
        }
      },
      "required": ["type", "contacts"]
    }
  ]
}
